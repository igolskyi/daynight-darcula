      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#C77DBB"
      }
    },
    {
      "name": "Git diff header",
      "scope": "meta.diff.header.git",
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "name": "Git diff header from/to file",
      "scope": "meta.diff.header.from-file, meta.diff.header.to-file",
      "settings": {
        "foreground": "#FFC66D"
      }
